{% extends 'base.html.twig' %}

{% block title %}Categorie{% endblock %}

{% block body %}

    {% include("menus/navigation_admin.html.twig") %}
    {% include("headers/header.html.twig") %}
    <div class="container">
        <div class="row">	
            <h1>Categorie : {{ categorie.titre }} </h1>
            <hr style="width:100%; height:10px; background-color:#1B4F72 ">
            <table class="table">
                <tbody>
                    <tr>
                        <th>Id</th>
                            <td>{{ categorie.id }}</td>
                        </tr>
                    <tr>
                    <th>Titre</th>
                        <td>{{ categorie.titre }}</td>
                    </tr>
                    <tr>
                        <th>Resume</th>
                        <td>{{ categorie.resume }}</td>
                    </tr>
                </tbody>
            </table>
           
        </div>
    </div>

{% endblock %}
